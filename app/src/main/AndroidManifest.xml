<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/book_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PMEUEpocaEspecial"
        tools:targetApi="31">
        <activity
            android:name=".activities.admin.AddProjectActivity"
            android:exported="false"
            android:label="@string/title_activity_add_project"
            android:theme="@style/Theme.PMEUEpocaEspecial" />
        <activity
            android:name=".activities.user.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.user.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="com.example.ACTION_LOGOUT"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.user.RegisterActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="com.example.lab002.SEG"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.user.UserEditActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="com.example.USER_UPDATE"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.user.ProfileActivity"
            android:exported="true" />
        <activity
            android:name=".activities.user.ProfileEditActivity"
            android:exported="false" />
        <activity
            android:name=".activities.user.ProjectActivity"
            android:exported="false">
            <intent-filter>
                <action android:name="com.example.lab004.SEG"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

    </application>

</manifest>